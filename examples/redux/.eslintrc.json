{
  "parser": "babel-eslint",
  "env": {
    "commonjs": true,
    "browser": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "parserOptions": {
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": ["react"],
  "extends": ["airbnb", "plugin:react/recommended"],
  "rules": {
    "key-spacing": 0,
    "react/jsx-no-bind":0,
    "react/prefer-es6-class": 0,
    "react-multi-comp": 0,
    "react/prefer-stateless-function": 1,
    "react/prop-types": 0,
    "react/jsx-filename-extension":0,
    "react/forbid-prop-types": 0,
    "react/no-string-refs": 0,
    "class-methods-use-this": 0,
    "object-shorthand": 2,
    "no-restricted-syntax": 0,
    "prefer-const": 1,
    "prefer-template": 1,
    "guard-for-in": 0,
    "max-len": [2, 150],
    "react/no-multi-comp": 0,
    "no-unused-vars": [2, {
      "args": "none"
    }],
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-return-assign": 1,
    "no-confusing-arrow": 1,
    "no-loop-func": 1,
    "prefer-arrow-callback": 1,
    "no-alert": 2,
    "no-caller": 2,
    "no-debugger": 2,
    "comma-dangle": 0,
    "func-names": 0,
    "operator-assignment": 0,
    "one-var": 0,
    "no-else-return": 0,
    "radix": 0,
    "no-console": 0,
    "one-var-declaration-per-line": 0,
    "consistent-return": 0,
    "no-var": 0,
    "quotes": 0,
    "no-plusplus": 0
  },
  "settings":{
    "import/parsers":{
      "typescript-eslint-parser": [ ".ts", ".tsx" ]
    }
  }
}